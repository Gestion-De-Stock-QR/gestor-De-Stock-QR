{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**/*.js", "dist/**/*.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main.js"
      }
    ]
  }
  